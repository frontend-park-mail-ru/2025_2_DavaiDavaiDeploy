{
  "name": "2025-2-davaidavaideploy",
  "version": "1.0.0",
  "description": "Репозиторий команды \"Davai Davai Deploy\". Проект: Кинопоиск",
  "type": "module",
  "private": true,
  "main": "index.tsx",
  "scripts": {
    "start": "vite --config vite/vite.config.dev.ts",
    "start:stage": "vite --config vite/vite.config.stage.ts",
    "start:prod": "vite --config vite/vite.config.prod.ts",
    "build": "tsc && vite build --config vite/vite.config.dev.ts",
    "build:stage": "tsc && vite build --config vite/vite.config.stage.ts --mode stage",
    "build:prod": "tsc && vite build --config vite/vite.config.prod.ts",
    "genTypes": "npx swagger-typescript-api generate -p ./swagger.yaml -o ./src/types -n models.ts --no-client",
    "test": "vitest --config vite/vite.config.dev.ts",
    "test:coverage": "vitest run --coverage",
    "tsc:check": "npx tsc --noEmit",
    "lint:eslint": "npx eslint .",
    "lint:eslint:fix": "npx eslint . --fix",
    "lint:prettier": "npx prettier . --check",
    "lint:prettier:fix": "npx prettier . --write",
    "lint:stylelint": "npx stylelint \"**/*.css\" --allow-empty-input",
    "lint:stylelint:fix": "npx stylelint \"**/*.css\" --fix --allow-empty-input",
    "prepare": "husky",
    "preview": "npm run build && vite preview",
    "preview:stage": "npm run build:stage && vite preview",
    "allure:generate": "npx allure generate allure-results --clean"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/frontend-park-mail-ru/2025_2_DavaiDavaiDeploy.git"
  },
  "browserslist": [
    "defaults",
    "not op_mini all",
    "not KaiOS > 0"
  ],
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/frontend-park-mail-ru/2025_2_DavaiDavaiDeploy/issues"
  },
  "homepage": "https://github.com/frontend-park-mail-ru/2025_2_DavaiDavaiDeploy#readme",
  "dependencies": {
    "@fontsource/golos-ui": "^4.5.3",
    "@robocotik/react": "^1.6.0",
    "@sentry/browser": "^10.21.0",
    "@sentry/core": "^10.21.0",
    "@sentry/vite-plugin": "^2.14.1",
    "@svgr/plugin-jsx": "^8.1.0",
    "@svgr/plugin-svgo": "^8.1.0",
    "@vkid/sdk": "^2.6.2",
    "cypress-real-events": "^1.15.0",
    "ddd-clsx": "^1.0.4",
    "ddd-ui-kit": "^1.1.0",
    "extendable-media-recorder": "^9.2.31",
    "extendable-media-recorder-wav-encoder": "^7.0.132",
    "reset-css": "^5.0.2",
    "vite-plugin-svgr": "^4.5.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.38.0",
    "@stylistic/eslint-plugin": "^5.5.0",
    "@types/node": "^24.7.2",
    "@vitest/coverage-v8": "^3.2.4",
    "allure-commandline": "^2.36.0",
    "allure-cypress": "^3.4.3",
    "browserslist": "^4.26.3",
    "cypress": "^15.7.1",
    "ember-template-lint": "^3.6.0",
    "eslint": "^9.38.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-compat": "^6.0.2",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-prettier": "^5.5.4",
    "eslint-plugin-sonarjs": "^3.0.5",
    "globals": "^16.4.0",
    "husky": "^9.1.7",
    "jsdom": "^27.0.0",
    "lint-staged": "^16.1.6",
    "prettier": "^3.6.2",
    "prettier-plugin-organize-imports": "^4.3.0",
    "reload": "^3.4.2",
    "sass": "^1.93.2",
    "stylelint": "^16.24.0",
    "stylelint-config-standard": "^39.0.0",
    "stylelint-config-standard-scss": "^16.0.0",
    "stylelint-scss": "^6.12.1",
    "swagger-typescript-api": "^13.2.15",
    "typescript": "~5.9.3",
    "typescript-eslint": "^8.46.1",
    "vite": "^7.1.7",
    "vite-plugin-pwa": "^1.1.0",
    "vite-plugin-static-copy": "^3.1.4",
